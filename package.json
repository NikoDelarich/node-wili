{
  "name": "wili",
  "version": "0.7.0",
  "description": "A Node client for the Wiener Linien public transport API",
  "author": "Jan T. Sott",
  "license": "MIT",
  "type": "module",
  "typings": "./types",
  "exports": {
    "import": "./lib/wili.mjs"
  },
  "engines": {
    "node": ">=16.13 || >=18"
  },
  "files": [
    "lib/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-wili.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run start",
    "lint:fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "prepack": "npm run build",
    "prepare": "husky install",
    "start": "npm run build -- --watch",
    "test": "uvu -r tsm tests"
  },
  "keywords": [
    "wiener linien",
    "wienerlinien",
    "vienna",
    "Ã¶pvn",
    "public transport"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rollup": "^3.27.0",
    "tslib": "^2.6.1",
    "tsm": "^2.3.0",
    "typescript": "^5.1.6",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
