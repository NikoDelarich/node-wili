    "@rollup/plugin-typescript": "^8.2.0",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "ava": "^3.15.0",
    "eslint": "^7.5.0",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "is-ci": "^2.0.0",
    "lint-staged": "^10.5.4",
    "rollup": "^2.23.0",
