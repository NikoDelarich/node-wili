{
  "name": "wili",
  "version": "0.5.1",
  "description": "A Node client for the Wiener Linien public transport API",
  "author": "Jan T. Sott",
  "license": "MIT",
  "type": "module",
  "exports": "./lib/wili.mjs",
  "typings": "types/",
  "engines": {
    "node": ">=16.13 || >=18"
  },
  "files": [
    "data/",
    "lib/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-wili.git"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=lib/wili.mjs --format=esm --external:isomorphic-fetch --external:query-string",
    "dev": "npm run start",
    "lint:fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "prepack": "npm run build",
    "prepare": "husky install",
    "start": "npm run build -- --watch",
    "test": "uvu tests"
  },
  "keywords": [
    "wiener linien",
    "wienerlinien",
    "vienna",
    "Ã¶pvn",
    "public transport"
  ],
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
