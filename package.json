{
  "name": "wili",
  "version": "0.5.1",
  "description": "A Node client for the Wiener Linien public transport API",
  "author": "Jan T. Sott",
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./lib/wili.mjs",
    "require": "./lib/wili.cjs"
  },
  "typings": "types/",
  "files": [
    "data/",
    "lib/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-wili.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run start",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "start": "rollup -w -c",
    "test": "ava ./test/test.*js --verbose --fail-fast",
    "prepare": "husky install"
  },
  "keywords": [
    "wiener linien",
    "wienerlinien",
    "vienna",
    "Ã¶pvn",
    "public transport"
  ],
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "ava": "^4.3.1",
    "encoding": "^0.1.13",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "rollup": "^3.20.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
