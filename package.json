{
  "name": "wili",
  "version": "0.5.1",
  "description": "A Node client for the Wiener Linien public transport API",
  "author": "Jan T. Sott",
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./lib/wili.mjs",
    "require": "./lib/wili.cjs"
  },
  "typings": "types/",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-wili.git"
  },
  "scripts": {
